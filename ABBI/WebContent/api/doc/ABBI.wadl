          </response>
        </method>
      </resource>
    </resource>
    <resource id="document" path="Document">
      <doc>
        <h:p>This resource represents a document in a patient health
          record.</h:p>
      </doc>
      <doc title="Conformance"><h:p>OPTIONAL</h:p></doc>
      <param name="Authorization" required="true" type="xs:string"
        style="header">
        <doc><h:p>Requires an OAuth 2.0 Token</h:p></doc>
      </param>
      <resource path="@{docId}">
        <doc>
          <h:p>This resource represents the specified document in the
            patient's health record.</h:p>
        </doc>
        <doc title="Conformance"><h:p>REQUIRED</h:p></doc>
